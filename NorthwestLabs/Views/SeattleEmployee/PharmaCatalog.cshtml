@model IEnumerable<NorthwestLabs.Models.Assay>

@{
    ViewBag.Title = "PharmaCatalog";
}

<h2>Pharmacology Catalog</h2>

<p>
    @Html.ActionLink("Create New", "CreateAssay")
</p>
<table class="table">
    <tr>
       
        <th>
            @Html.DisplayNameFor(model => model.Assay_Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Assay_Summary)
        </th>
        
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
       
        <td>
            @Html.DisplayFor(modelItem => item.Assay_Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Assay_Summary)
        </td>
        
        <td>
            @Html.ActionLink("Edit", "EditAssay", new { id=item.Assay_ID }) 
            @Html.ActionLink("Delete", "DeleteAssay", new { id=item.Assay_ID })
        </td>
    </tr>
}

</table>
