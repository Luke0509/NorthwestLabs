@model IEnumerable<NorthwestLabs.Models.Customer>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Cust_First_Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Cust_Last_Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Cust_Address)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Cust_City)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Cust_State)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Cust_Email)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Cust_Phone)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Account_Created_Date)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Cust_Discount)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Cust_First_Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Cust_Last_Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Cust_Address)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Cust_City)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Cust_State)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Cust_Email)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Cust_Phone)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Account_Created_Date)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Cust_Discount)
        </td>
        <td>
            @Html.ActionLink("Edit", "EditCustomer", new { id=item.Cust_ID }) |
            @Html.ActionLink("Details", "Details", new { id=item.Cust_ID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Cust_ID })
        </td>
    </tr>
}

</table>
